@{
    ViewData["Title"] = "Create Book";
}

<div class="text-center">
     <a asp-area="" asp-controller="Home" asp-action="CreateAuthorForm">Add Author</a>
    <ul>
        @foreach (Author author in Model)
        {
            <li style="list-style-type:none">
                <h2>@author.AuthorName</h2>
                <img width="400" height="400" src="@author.AuthorPhotoUrl" alt="@author.AuthorName" />
            </li>
        }
    </ul>
</div>